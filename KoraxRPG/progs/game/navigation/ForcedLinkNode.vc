class ForcedLinkNode : CommonNode
	__mobjinfo__(14999);

//
// ForcedLinkNode is a special kind of navigation node, that,
// apart from common node behavior, is forced to link with
// all Forced Link Nodes sharing same TID.
//

defaultproperties
{
	bNoGravity = true;
}
