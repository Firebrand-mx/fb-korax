class CentaurFX : Projectile;

states
{
Spawn:
	CTFX A -1 Bright
	Stop
Death:
	CTFX B 4 Bright
	CTFX C 3 Bright
	CTFX D 4 Bright
	CTFX E 3 Bright
	CTFX F 2 Bright
	Stop
}

defaultproperties
{
	Speed = 700.0;
	MissileDamage = 4;
	RenderStyle = STYLE_Add;
	DeathSound = 'CentaurMissileExplode';
	bDynamicLight = true;
	DLightColour = RGB(41, 119, 233);
	DLightRadius = 134.0;
}
