class MenuStaticBitmap : MenuObject;

name PicName;
int Handle;

void InitWindow()
{
	::InitWindow();
	Disable();
	if (PicName)
	{
		Handle = R_RegisterPic(PicName);
	}
}

void DrawWindow()
{
	DrawPic(0, 0, Handle);
}

defaultproperties
{
}
