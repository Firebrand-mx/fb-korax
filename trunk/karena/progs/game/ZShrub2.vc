class ZShrub2 : Actor
	__mobjinfo__(8102);

//==========================================================================
//
//	GetExplodeParms
//
//==========================================================================

void GetExplodeParms(out int damage, out float distance, out byte damageSelf)
{
	damage = 30;
	distance = 64.0;
}

states
{
Spawn:
	SHB2 A -1
	Stop
Death.Fire:
	SHB2 B 7 Bright
	SHB2 C 6 Bright A_Scream
	SHB2 D 5 Bright A_Explode
	SHB2 E 5 Bright
	Stop
}

defaultproperties
{
	Health = 10;
	Radius = 16.0;
	Height = 40.0;
	Mass = 99999.0;
	bSolid = true;
	bShootable = true;
	bNoBlood = true;
	DeathSound = 'TreeExplode';
}
