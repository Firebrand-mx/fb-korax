class FireThing2 : Decoration
	__mobjinfo__(15026);

states
{
Spawn:
	FSK2 A 4 Bright
	FSK2 B 3 Bright
	FSK2 C 4 Bright
	FSK2 D 3 Bright
	FSK2 E 4 Bright
	FSK2 F 3 Bright
	FSK2 G 4 Bright
	FSK2 H 3 Bright
	FSK2 I 4 Bright
	Loop
}

defaultproperties
{
	Radius = 5.0;
	Height = 10.0;
	bSolid = true;
	bStaticLight = true;
	LightColour = RGB(0, 0, 255);
	LightOffset = vector(0.0, 0.0, 16.0);
	LightRadius = 160.0;
}
