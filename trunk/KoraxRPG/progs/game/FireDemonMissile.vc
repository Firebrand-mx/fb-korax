class FireDemonMissile : Projectile;

states
{
Spawn:
	FDMB A 5 Bright
	Loop
Death:
	FDMB BCDE 5 Bright
	Stop
}

defaultproperties
{
	Radius = 10.0;
	Height = 6.0;
	Mass = 15.0;
	Speed = 350.0;
	MissileDamage = 1;
	RenderStyle = STYLE_Add;
	bFloorClip = true;
	DamageType = 'Fire';
	DeathSound = 'FireDemonMissileHit';
}
