class MenuChoice_DeathMatch : MenuChoiceEnum;

MenuChoice Level;
MenuChoice Monsters;

int GetInitialValue()
{
	return 1;
}

bool SetNewValue(int newValue)
{
	if (Level)
		MenuChoice_Level(Level).InitMapList(newValue);
	MenuChoice_Monsters(Monsters).InitMonsters(newValue);
	return ::SetNewValue(newValue);
}

defaultproperties
{
	Text = "Game type";
	EnumNames[0] = "Castle Sweep";
	EnumNames[1] = "Deathmatch";
	EnumNames[2] = "Team Deathmatch";
	EnumNames[3] = "Capture The Flag";
	EnumNames[4] = "Football";
	EnumNames[5] = "Team Castle Sweep";
}

