class ZGemPedestal : Decoration
	__mobjinfo__(9012);

__states__
{
	S_ZGEMPEDESTAL1('GMPD', 0, -1.0, S_NULL) { }
	S_ZGEMPEDESTAL2('GMPD', 1, -1.0, S_NULL) { }
}

//==========================================================================
//
//	Activate
//
//==========================================================================

bool Activate()
{
	SetState(S_ZGEMPEDESTAL2);
	return true;
}

defaultproperties
{
	Radius = 10.0;
	Height = 40.0;
	bSolid = true;
	IdleState = S_ZGEMPEDESTAL1;
}

