class HornRodMissile1 : Projectile;

states
{
Spawn:
	FX00 AB 6 Bright
	Loop
Death:
	FX00 HI 5 Bright
	FX00 JK 4 Bright
	FX00 LM 3 Bright
	Stop
}

defaultproperties
{
	Radius = 12.0;
	Height = 8.0;
	Speed = 770.0;
	MissileDamage = 5;
	bWindThrust = true;
	bActivateImpact = true;
	DamageType = 'Fire';
	SightSound = 'SkullRodFire';
	DeathSound = 'SkullRodHit';
	bDynamicLight = true;
	DLightColour = RGB(255, 74, 74);
	DLightRadius = 126.0;
	Obituary = "$ob_hellstaff";
}
